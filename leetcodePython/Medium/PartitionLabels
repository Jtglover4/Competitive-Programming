class Solution:
    def partitionLabels(self, s: str) -> List[int]:
        
        results = []
        curr = []

        mappy = Counter(s)
        temp =0

        for letter in s:

            if(letter not in curr):
                curr.append(letter)
            
            mappy[letter] -= 1

            temp += 1

            for j in curr:
                if(mappy[j] == 0):
                    curr.remove(j)
            if(len(curr) == 0):
                results.append(temp)
                temp = 0
                curr = []

        return results
